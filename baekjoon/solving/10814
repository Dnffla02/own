#include <iostream>
#include <algorithm>
#include <string>

using namespace std;

class User{
private:
	string name;
public:
	int num;
	int rank;
	User();
	~User();
	void getInfo(int getNum, string getName);
	void minMax_algorithm();
	void print();
};

User::User(){
	rank = 0;
}

User::~User(){

}

void User::print(){
	cout << num + " " + name << endl;
}

int main(){
	int a;
	cin >> a;
	int number;
	string name;
	User user[100001];
	for (int i = 0; i<a; i++){
		cin >> number;
		cin >> name;
		user[i].getInfo(number, name);
	}
	sort(user, user + a);
	for (int i = 0; i < a; i++){
		name[i].print();
	}
	return 0;
}
