#include "stdafx.h"

#include <iostream>

using namespace std;

int main(){
	int a;
	int i;
	cin >> a;
	int b[10] = { 0, };
	int c;
	for (i = 1; i <= a; i++){
		c = i;
		while (c != 0){
			b[c % 10]++;
			c /= 10;
		}
	}
	for (i = 0; i < 10; i++){
		cout << b[i]<<" ";
	}
	return 0;
}
/*
	100 
		11 21, 20
	200
		31 140 41, 40
	1000
		192 301, 300
	10000
		2893 4001, 4000
*/
