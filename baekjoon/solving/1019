#include "stdafx.h"

#include <iostream>

using namespace std;

int main(){
	int a;
	int i;
	cin >> a;
	int b[10] = { 0, };
	int c;
	for (i = 1; i <= a; i++){
		c = i;
		while (c != 0){
			b[c % 10]++;
			c /= 10;
		}
	}
	for (i = 0; i < 10; i++){
		cout << b[i]<<" ";
	}
	return 0;
}
/*
	
	4*1 4
	3*2 6
	2*3 6 
	1*4 4

	4000
	 600
	  60
	   4
	
	1234      3457
	 234      457
	  34      57
	   4      7

*/
